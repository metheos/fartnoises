{
  "functions": {
    "src/pages/api/socket.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/socket"
    }
  ],
  "buildCommand": "node scripts/download-assets.js && npm run build",
  "env": {
    "NEXT_PUBLIC_SOUND_BASE_URL": "/sounds"
  }
}
